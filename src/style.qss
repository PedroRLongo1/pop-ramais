    border-radius: 5px;
}
#buttonBase:hover {
    background-color: #007A7A;
    color: black;
}
#inputBase {
    background-color: #0B5B5B;
    color: white;
    border: 1px solid black;
    border-radius: 5px;
}
#inputBase:hover {
    background-color: #007A7A;
}


