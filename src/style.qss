/* Widgets */
#widgetpanelUp {
    background-color: #0F3B3B;
}
#widgetPanelDown {
    width: 100px; height: 100px;
    background-color: #0F3B3B;
}
#widgetPanel {
    background-color: rgb(90, 90, 90);
}
