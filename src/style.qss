}
/* labels */
#labelSearchTextArea {
    background-color: #0F3B3B;
    color: white;
    border: 1px solid white;
}
#labelEditTextArea {
    color: white;
}
#labelConvertTextArea {
    background-color: #0F3B3B;
    color: white;
    border: 1px solid white;
}
#labelPanelOption {
    background-color: #52DADA;
    color:black;
}
#title_modes{
    background-color: transparent;
    color: white;
    font-size: 20px
}
#subtitle_modes{
    background-color: transparent;
    color: white;
}
/* table */
#table {
    background-color: #52DADA;
    border: 1px solid black;
    border-radius: 5px;
}
#table::item {
    padding: 1px;
    border-bottom: 1px solid black;
    border-right: 1px dashed black;
}
#table::item:selected {
    background-color: #7DFAFA;
    color: black;
    font-weight: 100;
}
/* bases */
#buttonBase {
    background-color: #0B5B5B;
    color: white;
